<%- include('../partials/head.ejs')%>

  <head>
    <script>
      let dayName = new Array(
        "domingo",
        "segunda",
        "terça",
        "quarta",
        "quinta",
        "sexta",
        "sabado"
      );
      let monName = new Array(
